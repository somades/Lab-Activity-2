def compute_cubes(array):
    cubes = [x**3 for x in array]
    return cubes

size = int(input("Enter the size of the array: "))

array = list(map(int, input("Enter the elements separated by space: ").split()))

cubes = compute_cubes(array)
for cube in cubes:
    print(cube)
